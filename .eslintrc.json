{
  "extends": ["airbnb"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "import/no-unresolved": 0,
    "no-console": "off",
    "no-debugger": "off",
    "no-unused-vars": "off",
    "no-undef": "off",
    "no-redeclare": "off",
    "prefer-const": "off",
    "react/jsx-filename-extension": {
      "extensions": [
        // ".jsx",
        ".tsx"
      ]
    }
  },
  "env": {
    "shared-node-browser": true,
    "node": true,
    "es6": true
  }
}