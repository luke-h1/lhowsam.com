{
  "name": "lhowsam.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "test:update": "jest -u",
    "jest:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "stats": "ANALYZE=1 next build && cat .next/stats.txt",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.1.1",
    "axios": "^0.21.1",
    "date-fns": "^2.19.0",
    "framer-motion": "^4.0.3",
    "globby": "^11.0.3",
    "gray-matter": "^4.0.2",
    "next": "10.1.2",
    "next-mdx-remote": "^2.1.3",
    "next-seo": "^4.23.0",
    "polished": "^4.1.1",
    "prettier": "^2.2.1",
    "prism-react-renderer": "^1.2.0",
