{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "alphabetize": "node scripts/alphabetizePackageJson.js",
    "analyze": "ANALYZE=true next build && node scripts/analyze-bundle.js",
    "build": "next build",
    "check:node": "check-node-version --package",
    "clean": "rm -rf node_modules",
    "commit": "cz",
    "dev": "NODE_OPTIONS='-r newrelic' next dev -p 3000",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "prepare": "husky",
    "start": "NODE_OPTIONS='-r newrelic' next start -p 3000",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:snapshot": "jest -u",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "uitest:local": "make e2e-local"
  },
