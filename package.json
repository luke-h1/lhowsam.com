{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/jest": "^26.0.20",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.18.0",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-canonical-urls": "^2.8.0",
    "gatsby-plugin-catch-links": "^2.8.0",
    "gatsby-plugin-feed": "^2.11.0",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.9.0",
    "gatsby-plugin-manifest": "^2.10.0",
    "gatsby-plugin-mdx": "^1.8.0",
    "gatsby-plugin-offline": "^3.8.0",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sharp": "^2.12.2",
    "gatsby-plugin-sitemap": "^2.10.0",
    "gatsby-plugin-styled-components": "^3.8.0",
    "gatsby-plugin-theme-ui": "^0.3.5",
    "gatsby-plugin-twitter": "^2.8.0",
    "gatsby-plugin-typescript": "^2.10.0",
    "gatsby-remark-autolink-headers": "^2.9.0",
    "gatsby-remark-copy-linked-files": "^2.8.0",
    "gatsby-remark-images": "^3.9.0",
    "gatsby-remark-prismjs": "^3.11.0",
    "gatsby-remark-responsive-iframe": "^2.9.0",
    "gatsby-remark-smartypants": "^2.8.0",
    "gatsby-source-filesystem": "^2.9.1",
    "gatsby-source-strapi": "0.0.12",
    "gatsby-transformer-json": "^2.9.0",
    "gatsby-transformer-remark": "^2.14.0",
    "gatsby-transformer-sharp": "^2.10.1",
    "husky": "^4.3.8",
    "polished": "^4.0.5",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "styled-components": "^5.2.1",
    "theme-ui": "^0.3.5",
    "ts-jest": "^26.4.4"
  },
  "devDependencies": {
    "prettier": "2.2.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2"
  },
  "keywords": [
    "luke howsam",
    "lhowsam.com",
    "portfolio"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "NODE_ENV=development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "NODE_ENV=production npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint \"src/**/*.{js, jsx,ts,tsx}\" --quiet",
    "lint:fix": "eslint \"src/**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "clean": "gatsby clean",
    "test": "NODE_ENV=testing jest --silent",
    "test:update": " NODE_ENV=testing jest --silent -u",
    "deep-clean": "npm run clean && rm -rf node_modules package-lock.json && npm i"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
