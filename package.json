{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "vercel build",
    "vercel:dev": "vercel dev",
    "deploy:dev": "vc -f -d",
    "deploy:prod": "vc -f --prod -d",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "cy:ci": "npm run build && npx start-server-and-test http://localhost:3000 cy:run",
    "cy:run": "./node_modules/.bin/cypress run",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:prod": "CYPRESS_BASE_URL=https://lhowsam.com ./node_modules/.bin/cypress run",
    "cy:prod-open": "CYPRESS_BASE_URL=https://lhowsam.com ./node_modules/.bin/cypress open",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install",
    "check:node": "check-node-version --node 16 --npm 8",
    "preinstall": "npm run check:node",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.8",
    "@mapbox/rehype-prism": "^0.8.0",
    "@next/eslint-plugin-next": "^12.2.3",
    "@sanity/client": "^3.3.3",
    "@sanity/image-url": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "axios": "^0.27.2",
    "check-node-version": "^4.2.1",
    "classnames": "^2.3.1",
    "clsx": "^1.2.1",
    "cypress": "10.4.0",
    "cypress-multi-reporters": "^1.6.1",
    "date-fns": "^2.29.1",
    "framer-motion": "^6.5.1",
    "gen-env-types": "^1.3.4",
    "groq": "2.29.3",
    "lodash": "^4.17.21",
    "mdx-prism": "^0.3.4",
    "next": "^12",
    "next-mdx-remote": "^4.1.0",
    "next-seo": "^5.5.0",
    "next-sitemap": "3.1.16",
    "next-transpile-modules": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.1.0",
    "rehype-prism-plus": "^1.4.2",
    "rehype-slug": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sass": "^1.54.1",
    "sharp": "^0.30.7",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^14.4.1",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.181",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^14.0.0",
    "jest-resolve": "^28.1.3",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^2.0.0",
    "lint-staged": "13.0.3",
    "prettier": "^2.7.1",
    "react-icons": "^4.4.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 10,
    "showDetails": true
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  }
}
