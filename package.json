  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy:dev": "gatsby clean && gatsby build && vc",
    "deploy:prod": "gatsby clean && gatsby build && vc --prod",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format:styles": "prettier '**/*.{css,scss}' --write",
    "start": "NODE_ENV=production npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint \"src/**/*.{js, jsx,ts,tsx}\" --quiet",
    "lint:fix": "eslint \"src/**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "test": "NODE_ENV=testing  jest --silent",
    "jest:update": " NODE_ENV=testing jest --silent -u",
    "test:coverage": "NODE_ENV=testing jest --coverage ",
    "test:watch": "NODE_ENV=testing jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "test:e2e:dev": "cross-env CYPRESS_SUPPORT=y start-server-and-test dev http://localhost:8000 cy:open",
    "test:e2e:run": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "cross-env CYPRESS_SUPPORT=y start-server-and-test develop http://localhost:8000 cy:run:ci",
    "dev": "NODE_ENV=development gatsby develop",
    "dev-mobile": "gatsby develop -H 0.0.0.0",
    "deep-clean": "npm run clean && rm -rf node_modules package-lock.json && npm i"
  },
