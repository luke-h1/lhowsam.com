{
  "name": "@lhowsam/frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "cy:open": "./node_modules/.bin/cypress open",
    "cy:prod": "CYPRESS_BASE_URL=https://lhowsam.com ./node_modules/.bin/cypress run",
    "cy:prod-open": "CYPRESS_BASE_URL=https://lhowsam.com ./node_modules/.bin/cypress open",
    "cy:run": "./node_modules/.bin/cypress run",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@mdx-js/react": "^1.6.22",
    "@next/eslint-plugin-next": "^12.0.7",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "classnames": "^2.3.1",
    "core-js": "^3.20.0",
    "cypress": "9.2.0",
    "date-fns": "^2.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gen-env-types": "^1.3.0",
    "graphql-request": "^3.7.0",
    "gray-matter": "^4.0.3",
    "hast-util-to-html": "^8.0.3",
    "hast-util-to-string": "^2.0.0",
    "mdx-prism": "^0.3.4",
    "next": "^12.0.7",
    "next-mdx-remote": "^3.0.8",
    "next-seo": "^4.28.1",
    "next-sitemap": "^1.6.203",
    "parse-numeric-range": "^1.3.0",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.25.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-spring": "^9.3.2",
    "rehype": "^12.0.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.0.3",
    "rehype-parse": "^8.0.3",
    "rehype-slug": "^5.0.1",
    "remark-autolink-headings": "^7.0.1",
    "remark-code-titles": "^0.1.2",
    "remark-gfm": "^3.0.1",
    "remark-slug": "^6.1.0",
    "sharp": "^0.29.3",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "unified": "^10.1.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.4.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^17.0.7",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.37",
    "@types/testing-library__dom": "^7.5.0",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^27.4.4",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^11.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "jest-resolve": "^27.4.4",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "react-icons": "^4.3.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.(test).{js,jsx,ts,tsx}",
      "<rootDir>/__tests__/**/*.(test).{js,jsx,ts,tsx}",
      "<rootDir>/src/components/__tests__/**/*.(test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/test/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/test/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@src/(.*)$": "<rootDir>/src/$1",
      "^react$": "<rootDir>/node_modules/react"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ],
    "resetMocks": true,
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  }
}
