{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "alphabetize": "node scripts/alphabetizePackageJson.js",
    "commit": "cz",
    "clean": "rm -rf node_modules",
    "check:node": "check-node-version --package",
    "dev": "next dev",
    "dev:prod": "nodemon --legacy-watch",
    "build": "next build",
    "build:prod": "next build && tsc --project tsconfig.server.json",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest -u",
    "analyze": "ANALYZE=true next build && node scripts/analyze-bundle.js",
    "prepare": "husky",
    "preinstall": "pnpm check:node"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.0.14",
    "@mdx-js/mdx": "^3.0.1",
    "@newrelic/next": "^0.9.0",
    "@next/eslint-plugin-next": "^14.2.3",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@reduxjs/toolkit": "^2.2.3",
    "@sanity/client": "^6.17.2",
    "@sanity/image-url": "^1.0.2",
    "@shikijs/rehype": "^1.3.0",
    "@vanilla-extract/css": "^1.15.1",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/next-plugin": "^2.4.0",
    "@vanilla-extract/recipes": "^0.5.2",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vercel/otel": "^1.8.2",
    "axios": "^1.6.8",
    "check-node-version": "^4.2.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cross-env": "^7.0.3",
    "csstype": "^3.1.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.1.7",
    "gen-env-types": "^1.3.4",
    "gray-matter": "^4.0.3",
    "groq": "3.40.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "motion": "^10.17.0",
    "nanoid": "^5.0.6",
    "newrelic": "^11.16.0",
    "next": "^14.2.3",
    "next-mdx-remote": "^4.4.1",
    "next-sanity": "^9.0.13",
    "next-seo": "^6.5.0",
    "next-themes": "^0.3.0",
    "nodemon": "^3.1.0",
    "nprogress": "^0.2.0",
    "posthog-js": "^1.130.2",
    "posthog-node": "^4.0.1",
    "prom-client": "^15.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "react-ga4": "^2.1.0",
    "react-icons": "^5.2.0",
    "react-lite-youtube-embed": "^2.4.0",
    "react-redux": "^9.1.2",
    "react-wrap-balancer": "^1.1.0",
    "redux": "^5.0.1",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-code-titles": "^1.2.0",
    "rehype-highlight": "^7.0.0",
    "rehype-prism-plus": "^2.0.0",
    "remark-gfm": "^4.0.0",
    "remark-highlight.js": "^7.0.1",
    "remark-prism": "^1.3.6",
    "rss": "^1.2.2",
    "sass": "^1.76.0",
    "sharp": "^0.33.3",
    "shiki": "^1.3.0",
    "swr": "^2.2.5",
    "the-new-css-reset": "^1.11.2",
    "tinykeys": "^2.1.0",
    "unist-util-visit": "^5.0.0",
    "use-roving-index": "^2.0.0",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@playwright/test": "^1.43.0",
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.5.0",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/jest": "^29.5.12",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.1",
    "@types/node": "20.12.8",
    "@types/nprogress": "^0.2.3",
    "@types/react": "18.3.1",
    "@types/rss": "^0.0.32",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "git-cz": "^4.9.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-matcher-utils": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "jest-resolve": "^29.7.0",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "15.2.2",
    "node-mocks-http": "^1.14.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.4",
    "vercel": "^34.1.8"
  },
  "nextBundleAnalysis": {
    "budget": 204800,
    "budgetPercentIncreaseRed": 10,
    "minimumChangeThreshold": 0,
    "showDetails": true
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ],
    "package.json": [
      "node scripts/alphabetizePackageJson.js"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}