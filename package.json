{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "alphabetize": "node ./scripts/alphabetizePackageJson.js",
    "clean": "rm -rf node_modules",
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "vercel build",
    "vercel:dev": "vercel dev",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "gen": "graphql-codegen --require dotenv/config --config codegen.ts",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "lint:style": "stylelint --fix src/**/*.{scss,css}",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "e2e:local-ci": "BASE_URL='http://localhost:3000' ./node_modules/.bin/playwright test --config=playwright-ci.config.ts",
    "e2e:local": "BASE_URL='http://localhost:3000' ./node_modules/.bin/playwright test --config=playwright-common.config.ts",
    "e2e:local-open": "BASE_URL='http://localhost:3000' ./node_modules/.bin/playwright test --headed --config=playwright-common.config.ts",
    "e2e:dev": "BASE_URL='https://dev.lhowsam.com' ./node_modules/.bin/playwright test --config=playwright-dev.config.ts",
    "e2e:dev-open": "BASE_URL='https://dev.lhowsam.com' ./node_modules/.bin/playwright test  --headed --config=playwright-dev.config.ts",
    "e2e:prod": "BASE_URL='https://lhowsam.com' ./node_modules/.bin/playwright test --config=playwright-common.config.ts",
    "e2e:prod-open": "BASE_URL='https://lhowsam.com' ./node_modules/.bin/playwright test --headed  --config=playwright-common.config.ts",
    "e2e:install-browsers": "./node_modules/.bin/playwright install --with-deps chromium",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest -u",
    "analyze": "ANALYZE=true next build",
    "check:node": "check-node-version --node 18 --pnpm 8",
    "preinstall": "pnpm run check:node",
    "postbuild": "next-sitemap",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.0.7",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@next/eslint-plugin-next": "^13.4.12",
    "@tanstack/react-query": "4.32.6",
    "axios": "^1.4.0",
    "check-node-version": "^4.2.1",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "gen-env-types": "^1.3.4",
    "graphql-request": "^6.1.0",
    "gray-matter": "^4.0.3",
    "groq": "3.15.1",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "next": "^13.4.12",
    "next-mdx-remote": "^4.4.1",
    "next-seo": "^6.1.0",
    "next-sitemap": "4.2.2",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-ga4": "^2.1.0",
    "react-icons": "^4.10.1",
    "react-lite-youtube-embed": "^2.3.52",
    "react-wrap-balancer": "^1.0.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-code-titles": "^1.2.0",
    "rehype-highlight": "^6.0.0",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sass": "^1.64.2",
    "sharp": "^0.32.4",
    "unist-util-visit": "^5.0.0",
    "use-roving-index": "^2.0.0",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@playwright/test": "^1.36.2",
    "@svgr/webpack": "^8.0.1",
    "@swc/core": "^1.3.74",
    "@swc/jest": "^0.2.27",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.196",
    "@types/node": "20.4.10",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.2.20",
    "@types/rss": "^0.0.30",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.12",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.7.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-junit": "^16.0.0",
    "jest-matcher-utils": "^29.6.2",
    "jest-mock-extended": "^3.0.5",
    "jest-resolve": "^29.6.2",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "13.2.3",
    "prettier": "^3.0.1",
    "stylelint": "^15.10.2",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vercel": "^31.2.2"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 10,
    "showDetails": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ],
    "package.json": [
      "node scripts/alphabetizePackageJson.js"
    ]
  }
}