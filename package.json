{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "alphabetize": "node scripts/alphabetizePackageJson.js",
    "analyze": "ANALYZE=true next build && node scripts/analyze-bundle.js",
    "build": "next build",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "check:node": "check-node-version --package",
    "clean": "rm -rf node_modules",
    "commit": "cz",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "dev": "next dev -p 3000",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "prepare": "husky",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "start": "next start -p 3000",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:snapshot": "jest -u",
    "test:watch": "jest --watch",
    "tsc": "tsc --noEmit",
    "uitest:local": "make e2e-local",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload"
  },
  "dependencies": {
    "@fontsource/poppins": "^5.1.1",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@mdx-js/mdx": "^3.1.0",
    "@next/eslint-plugin-next": "15.1.4",
    "@next/third-parties": "15.1.4",
    "@opennextjs/cloudflare": "1.0.0-beta.2",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@rehype-pretty/transformers": "^0.13.2",
    "@sanity/cli": "^3.68.3",
    "@sanity/client": "^6.24.1",
    "@sanity/dashboard": "^4.1.2",
    "@sanity/image-url": "^1.1.0",
    "@sanity/ui": "^2.11.1",
    "@sanity/vision": "3.68.3",
    "@tanstack/react-query": "^5.62.16",
    "@tanstack/react-query-next-experimental": "^5.62.16",
    "@vanilla-extract/css": "^1.17.1",
    "@vanilla-extract/css-utils": "^0.1.4",
