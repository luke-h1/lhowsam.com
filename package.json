    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleNameMapper": {
      "@src/(.*)$": "src/$1",
      "@tests/(.*)$": "./src/__tests__/$1",
      "@pages/(.*)$": "pages/$1",
      "@components/(.*)$": "src/components/$1",
      "@icons/(.*)$": "src/Icons/$1",
      "@utils/(.*)$": "src/utils/$1",
      "@styles/(.*)$": "src/styles/$1",
      "@types/(.*)$": "src/types/$1",
      "@templates/(.*)$": "src/templates/$1",
      "@data/(.*)$": "src/data/$1"
    }
  }
