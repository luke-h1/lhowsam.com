{
  "name": "root",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel:build": "vercel build",
    "vercel:dev": "vercel dev",
    "start": "next start",
    "tsc": "tsc --noEmit",
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "lint": "eslint \"./src/**/*.{js, jsx,ts,tsx}\" --quiet --fix && eslint \"./**/*.{js, jsx,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss,html}\"",
    "e2e:local": "BASE_URL='http://localhost:3000' ./node_modules/.bin/playwright test",
    "e2e:local-open": "BASE_URL='http://localhost:3000' ./node_modules/.bin/playwright test --headed",
    "e2e:preview": "BASE_URL='https://luke-h-portfolio-luke-h1.vercel.app' ./node_modules/.bin/playwright test --config=playwright-preview.config.ts",
    "e2e:preview-open": "BASE_URL='https://luke-h-portfolio-luke-h1.vercel.app' ./node_modules/.bin/playwright test  --headed --config=playwright-preview.config.ts",
    "e2e:prod": "BASE_URL='https://lhowsam.com' ./node_modules/.bin/playwright test --config=playwright-prod.config.ts",
    "e2e:prod-open": "BASE_URL='https://lhowsam.com' ./node_modules/.bin/playwright test --headed  --config=playwright-prod.config.ts",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "test:snapshot": "jest --updateSnapshot",
    "analyze": "ANALYZE=true next build",
    "prepare": "husky install",
    "check:node": "check-node-version --node 18.12.1 --pnpm 7",
    "preinstall": "npm run check:node",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@fontsource/poppins": "^4.5.10",
    "@mapbox/rehype-prism": "^0.8.0",
    "@next/eslint-plugin-next": "^13.0.6",
    "@sanity/client": "^3.4.1",
    "@sanity/image-url": "^1.0.1",
    "@tanstack/react-query": "4.19.1",
    "@vercel/analytics": "^0.1.5",
    "axios": "^1.2.1",
    "check-node-version": "^4.2.1",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^7.6.19",
    "gen-env-types": "^1.3.4",
    "groq": "2.33.2",
    "lodash": "^4.17.21",
    "mdx-prism": "^0.3.4",
    "next": "^13.0.6",
    "next-mdx-remote": "^4.2.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "3.1.42",
    "next-transpile-modules": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "reading-time": "^1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-code-titles": "^1.1.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "rss": "^1.2.2",
    "sass": "^1.56.1",
    "sharp": "^0.31.2",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@playwright/test": "^1.28.1",
    "@svgr/webpack": "^6.5.1",
    "@swc/core": "^1.3.21",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.14",
    "@types/react": "18.0.26",
    "@types/rss": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "jest-resolve": "^29.3.1",
    "jest-serializer-html": "^7.1.0",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "13.1.0",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.9.4",
    "vercel": "^28.8.0"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 10,
    "showDetails": true
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx,css,scss,html}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "defaultScope": "global"
    }
  }
}
